{
  "name": "esri-redux",
  "version": "0.1.0",
  "description": "Simple boilerplate demonstrating how to setup a project using React, Redux, Flow, and the Esri JavaScript API.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm run babel -- -w & gulp start & gulp serve",
    "secure": "npm run babel -- -w & gulp start & SERVER=https gulp serve",
    "dist": "BABEL_ENV=production gulp dist; npm run optimize;",
    "babel": "babel src --out-dir build",
    "test": "eslint src/js/**/*.js; flow check;",
    "postinstall": "bower install;",
    "optimize": "r.js -o rjs.build.js"
  },
  "keywords": [
    "esri",
    "react",
    "redux",
    "es6",
    "flux",
    "flow",
    "javascript"
  ],
  "author": "RobertW",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bower": "^1.7.9",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "browser-sync": "^2.12.8",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "flow-bin": "^0.25.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.1",
    "requirejs": "^2.2.0"
  }
}
